{
    "name": "lembarek/role",
    "description": "to add the roles system to laravel application",
    "keywords": [
        "lembarek",
        "role"
    ],
    "homepage": "https://github.com/lembarek/role",
    "license": "MIT",
    "authors": [
        {
            "name": "benghezala lembarek",
            "email": "lembarek2011@gmail.com",
