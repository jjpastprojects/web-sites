{
    "name": "{{vendor}}/{{name}}",
    "description": "{{description}}",
    "keywords": [
        "{{vendor}}",
        "{{name}}"
    ],
    "homepage": "https://github.com/{{vendor}}/{{name}}",
    "license": "MIT",
    "authors": [
        {
            "name": "{{author_name}}",
            "email": "{{author_email}}",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.2",
        "php" : ">=5.3.0",
        "lembarek/core" : "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "codeception/codeception": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "{{Vendor}}\\{{Name}}\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
