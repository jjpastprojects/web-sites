<h1>New challenge</h1>
<h3>{{= (challenge.challenger.author || challenge.challenger.guest).name }} challenged at <a href="{{= url }}">{{= siteName}}: {{= conversationTitle}}</a></h3>
<p>
    {{ if(challenge.challenger.ptext) { }}
        {{- challenge.challenger.ptext }}
    {{ } else { }}
        {{= challenge.challenger.text }}
    {{ } }}
</p>
<p class="bodyFooter">
    <% include subscription_footer_with_token_html %>
    <br>
    <% include burnzonefooter_html %>
</p>
                                            
